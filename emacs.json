{
  "title": "Emacs Specific Modifications",
  "rules": [
    {
      "description": "right_alt + right_shift for right_control",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "right_alt"
              },
              {
                "key_code": "right_shift"
              }
            ]
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right_alt + left_arrow for right_control",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "right_alt"
              },
              {
                "key_code": "left_arrow"
              }
            ]
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
