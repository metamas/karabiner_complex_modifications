{
  "title": "Emacs Specific Modifications",
  "rules": [
    {
      "type": "basic",
      "description": "Use right_alt + left_arrow for right_control",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "right_alt"
              },
              {
                "key_code": "left_arrow"
              }
            ]
          },
          "to": [
            {
              "key_code": "right_control"
            }
          ]
        }
      ]
    }
  ]
}
